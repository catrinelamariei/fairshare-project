<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="324.0" prefWidth="751.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.PrivCheckPageCtrl">
    <Button alignment="CENTER" layoutX="20.0" layoutY="182.0" mnemonicParsing="false" onAction="#requestCodeGeneration" prefHeight="66.0" prefWidth="286.0" text="Generate Code">
      <font>
         <Font size="26.0" />
      </font></Button>
    <PasswordField fx:id="password" layoutX="322.0" layoutY="182.0" onKeyPressed="#handleEnterPressed" prefHeight="66.0" prefWidth="286.0" promptText="Password">
      <font>
         <Font size="30.0" />
      </font></PasswordField>
    <Text fx:id="text" layoutX="14.0" layoutY="140.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please enter the code to login">
      <font>
         <Font size="39.0" />
      </font></Text>
   <Button alignment="CENTER" layoutX="622.0" layoutY="182.0" mnemonicParsing="false" onAction="#login" prefHeight="66.0" prefWidth="105.0" text="Login">
      <font>
         <Font size="26.0" />
      </font>
   </Button>
   <Button layoutX="685.0" layoutY="14.0" mnemonicParsing="false" onAction="#goToHome" text="Home" />
</AnchorPane>
